{
  "version": "0.2.0",
  "configurations": [
    {
      "name": ".NET-Lambda-Local",
      "type": "coreclr",
      "request": "attach",
      "processId": 1,
      "pipeTransport": {
        "pipeProgram": "docker",
        "pipeArgs": [
          "exec",
          "-i",
          "lambda-debug"
        ],
        "debuggerPath": "/vsdbg/vsdbg",
        "pipeCwd": "${workspaceFolder}/",
        "quoteArgs": false
      },
      "sourceFileMap": {
        "/build/": "${workspaceFolder}/"
      },
      "justMyCode": true,
      "symbolOptions": {
        "searchMicrosoftSymbolServer": true,
        "searchNuGetOrgSymbolServer": true
      }
    },
    {
      "name": ".NET-Lambda-Stage",
      "type": "debugpy",
      "request": "launch",
      "program": "${workspaceFolder}/infrastructure/run_local_cloud.py",
      "args": [
        "--environment",
        "stage"
      ],
      "cwd": "${workspaceFolder}",
      "console": "integratedTerminal"
    },
    {
      "name": ".NET-Local",
      "type": "coreclr",
      "request": "attach",
      "processId": 1,
      "pipeTransport": {
        "pipeProgram": "docker",
        "pipeArgs": [
          "exec",
          "-i",
          "debug-local"
        ],
        "debuggerPath": "/vsdbg/vsdbg",
        "pipeCwd": "${workspaceFolder}/",
        "quoteArgs": false
      },
      "sourceFileMap": {
        "/build/": "${workspaceFolder}/"
      },
      "justMyCode": true,
      "symbolOptions": {
        "searchMicrosoftSymbolServer": true,
        "searchNuGetOrgSymbolServer": true
      }
    }
  ]
}
